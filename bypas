function BPSPJK()
gg.clearResults()
gg.setRanges(gg.REGION_C_ALLOC)
gg.searchNumber('4502', gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)                                                                                                                                                                                                                                                                                --NRCLan
gg.refineNumber('4502', gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(999)
gg.editAll('4311', gg.TYPE_DWORD)
gg.clearResults()
gg.setRanges(gg.REGION_C_ALLOC)
gg.searchNumber('16384;4448', gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)                                                                                                                                                                                                                                                                                --NRCLan
gg.refineNumber('4448', gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(99900000)
gg.editAll('4311', gg.TYPE_DWORD)
if
gg.getResultCount() == 0
then
gg.alert('Bypass SPJK Failed ')
gg.alert('Silahkan Close PUBG dan Ulangi!')
os.exit()
else
gg.alert('Bypass SPJK Sukses Lets Play!')
gg.getResults(99900000)
gg.editAll('4311', gg.TYPE_DWORD)
gg.getResults(99900000)
local t = gg.getResults(99900000)
for i, v in ipairs(t) do
	if v.flags == gg.TYPE_DWORD then
		v.value = '1334'
		v.freeze = true
gg.addListItems(t)
t = nil
    end
  end
end
end

